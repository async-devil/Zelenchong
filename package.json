{
  "name": "zelenchong",
  "version": "1.0.0",
  "description": "Zelenchong bot source",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "npx nodemon src/index.js --watch src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/async-devil/Zelenchong.git"
  },
  "keywords": [
    "Discord",
    "Bot"
  ],
  "author": "async-devil",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/async-devil/Zelenchong/issues"
  },
  "homepage": "https://github.com/async-devil/Zelenchong#readme",
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "nodemon": "^2.0.7",
    "prettier": "2.1.2"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "tabWidth": 2,
    "semi": true
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "babel",
      "import",
      "plugin:prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "ignorePatterns": [
      "/.git",
      "/.vscode",
      "tsconfig.json",
      "node_modules"
    ],
    "rules": {
      "no-console": "off",
      "prettier/prettier": "error"
    }
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "/node_modules",
      "/.vscode"
    ],
    "ext": "js"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.2.7",
    "opusscript": "^0.0.7",
    "request": "^2.88.2",
    "ytdl-core": "^4.4.5"
  }
}
