{
  "name": "zelenchong",
  "version": "2.2.3",
  "description": "Zelenchong bot source",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "npx nodemon ./src/index.js --watch ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/async-devil/Zelenchong.git"
  },
  "keywords": [
    "Discord",
    "Bot"
  ],
  "author": "async-devil",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/async-devil/Zelenchong/issues"
  },
  "homepage": "https://github.com/async-devil/Zelenchong#readme",
  "devDependencies": {
    "@types/yt-search": "^2.3.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "nodemon": "^2.0.7",
    "prettier": "2.1.2"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "tabWidth": 2,
    "semi": true
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ],
    "rules": {
      "jsx-quotes": [
        1,
        "prefer-double"
      ],
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "internal",
            [
              "parent",
              "sibling"
            ]
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "external",
              "position": "before"
            }
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "/node_modules",
      "/.vscode"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts,json",
    "exec": "npx ts-node ./src/index.ts"
  },
  "dependencies": {
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.2.7",
    "opusscript": "^0.0.7",
    "yt-search": "^2.7.3",
    "ytdl-core": "^4.7.0",
    "ytfps": "^1.0.4"
  }
}